{
  "name": "@client-insight-backend/client-insight-backend-core-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@client-insight-backend/client-insight-backend-core"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/client-insight-backend-core-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@client-insight-backend/client-insight-backend-core:build",
          "@client-insight-backend/client-insight-backend-core:serve"
        ]
      }
    }
  }
}
